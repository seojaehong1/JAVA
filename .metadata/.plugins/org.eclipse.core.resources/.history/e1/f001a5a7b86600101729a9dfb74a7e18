		} else {
			System.out.println("0입니다.");
		}*/
		int grade = 0;
		if (num>=90) {
			grade = 1;
		} else if(num>=80) {
			grade = 2;
		} else if(num>=70) {
			grade = 3;
		} else{
			grade = 4;
			}
		
		
		
		/*
		if (num<0) {
			System.out.println("음수입니다.");
		} else if(num>0) {
