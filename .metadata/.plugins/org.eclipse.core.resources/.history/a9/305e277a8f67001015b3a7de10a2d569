			System.out.println("학생이름 입력");
			names[i] = sc.nextLine();
			System.out.println("성적 입력");
			score[i] = Integer.parseInt(sc.nextLine());
			System.out.println("학생이름 입력");
			grade[i] = sc.nextLine();
			
		}
		
		
